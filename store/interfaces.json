{
  "product-list": {
    "component": "ProductListWrapper",
    "allowed": ["product-list-content-desktop", "product-list-content-mobile"]
  },
  "product-list-content-desktop": {
    "component": "ProductList",
    "composition": "children",
    "allowed": "*"
  },
  "product-list-content-mobile": {
    "component": "ProductList",
    "composition": "children",
    "allowed": "*"
  },
  "product-list-image": {
    "component": "Image",
    "preview": {
      "type": "box",
      "height": 96,
      "width": 96,
      "options": {
        "padding": 0,
        "minWidth": 96
      }
    }
  },
  "product-list-message": {
    "component": "AvailabilityMessage"
  },
  "product-list-product-name": {
    "component": "ProductName",
    "preview": {
      "type": "box",
      "height": 30,
      "width": 140,
      "options": {
        "padding": 0
      }
    }
  },
  "product-list-product-brand": {
    "component": "ProductBrand",
    "preview": {
      "type": "box",
      "height": 15,
      "width": 110,
      "options": {
        "padding": 0
      }
    }
  },
  "product-list-product-variations": {
    "component": "ProductVariations"
  },
  "product-list-quantity-selector": {
    "component": "QuantitySelector",
    "preview": {
      "type": "box",
      "height": 35,
      "width": 80,
      "options": {
        "padding": 0
      }
    }
  },
  "product-list-unit-price": {
    "component": "UnitPrice"
  },
  "product-list-price": {
    "component": "Price",
    "preview": {
      "type": "box",
      "height": 35,
      "width": 90,
      "options": {
        "padding": 0
      }
    }
  },
  "product-list-remove-button": {
    "component": "RemoveButton",
    "preview": {
      "type": "box",
      "height": 25,
      "width": 25,
      "options": {
        "padding": 0
      }
    }
  }
}
