  "product-brand": {
    "component": "ProductBrand",
    "preview": {
      "type": "box",
      "height": 60,
      "width": 200
    }
  },
  "product-variations": {
    "component": "ProductVariations",
    "preview": {
      "type": "box",
      "height": 70,
      "width": 400
    }
  },
  "quantity-selector": {
    "component": "QuantitySelector",
    "preview": {
      "type": "box",
      "height": 80,
      "width": 200
    }
  },
  "unit-price": {
    "component": "UnitPrice",
    "preview": {
      "type": "box",
      "height": 80,
      "width": 200
    }
  },
  "price": {
    "component": "Price",
    "preview": {
      "type": "box",
      "height": 80,
      "width": 200
    }
  },
  "remove-button": {
    "component": "RemoveButton",
    "preview": {
      "type": "box",
      "height": 70,
      "width": 80
    }
  }
