{
  "product-list": {
    "component": "ProductListWrapper",
    "allowed": ["product-list-content-desktop", "product-list-content-mobile"]
  },
  "product-list-content-desktop": {
    "component": "ProductList",
    "composition": "children",
    "allowed": "*"
  },
  "product-list-content-mobile": {
    "component": "ProductList",
    "composition": "children",
    "allowed": "*"
  },
  "image": {
    "component": "Image",
    "preview": {
      "type": "box",
      "height": 96,
      "width": 96,
      "minWidth": 96
    }
  },
  "message": {
    "component": "AvailabilityMessage"
  },
  "product-name": {
    "component": "ProductName",
    "preview": {
      "type": "box",
      "height": 30,
      "width": 260
    }
  },
  "product-brand": {
    "component": "ProductBrand",
    "preview": {
      "type": "box",
      "height": 15,
      "width": 120
    }
  },
  "product-variations": {
    "component": "ProductVariations"
  },
  "quantity-selector": {
    "component": "QuantitySelector",
    "preview": {
      "type": "box",
      "height": 35,
      "width": 90
    }
  },
  "unit-price": {
    "component": "UnitPrice"
  },
  "price": {
    "component": "Price",
    "preview": {
      "type": "box",
      "height": 35,
      "width": 100
    }
  },
  "remove-button": {
    "component": "RemoveButton",
    "preview": {
      "type": "box",
      "height": 25,
      "width": 25
    }
  }
}
