{
  "product-list": {
    "component": "ProductListWrapper",
    "allowed": ["product-list-content-desktop", "product-list-content-mobile"]
  },
  "product-list-content-desktop": {
    "component": "ProductList",
    "composition": "children",
    "allowed": "*"
  },
  "product-list-content-mobile": {
    "component": "ProductList",
    "composition": "children",
    "allowed": "*"
  },
  "image": {
    "component": "Image",
    "preview": {
      "type": "box",
      "height": 200,
      "width": 200
    }
  },
  "message": {
    "component": "AvailabilityMessage"
  },
  "product-name": {
    "component": "ProductName",
    "preview": {
      "type": "box",
      "height": 70,
      "width": 400
    }
