{
  "product-list": {
    "children": ["flex-layout.row#list-row"]
  },
  "flex-layout.row#list-row": {
    "children": ["flex-layout.col#image", "flex-layout.col#container"],
    "props": {
      "colSizing": "auto",
      "preserveLayoutOnMobile": "true"
    }
  },
  "flex-layout.col#image": {
    "children": ["image"]
  },
  "flex-layout.col#container": {
    "children": ["container"],
    "props": {
      "width": "grow"
    }
  }
}
