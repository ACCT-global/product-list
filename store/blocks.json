{
  "product-list": {
    "children": ["flex-layout.row#list-row"]
  },
  "flex-layout.row#list-row": {
    "children": ["flex-layout.col#image", "flex-layout.col#container"],
    "props": {
      "colSizing": "auto",
      "preserveLayoutOnMobile": "true"
    }
  },
  "flex-layout.col#image": {
    "children": ["image"]
  },
  "flex-layout.col#container": {
    "children": ["flex-layout.row#container2", "message"],
    "props": {
      "width": "grow"
    }
  },
  "flex-layout.row#container2": {
    "children": ["flex-layout.col#product-description", "container"],
    "props": {
      "preserveLayoutOnMobile": "true",
      "colSizing": "auto"
    }
  },
  "flex-layout.col#product-description": {
    "children": ["product-description"],
    "props": {
      "width": "grow"
    }
  }
}
