{
  "product-list": {
    "children": ["flex-layout.row#list-row"]
  },
  "flex-layout.row#list-row": {
    "children": ["flex-layout.col#image", "flex-layout.col#container.mobile"],
    "props": {
      "colSizing": "auto",
      "preserveLayoutOnMobile": "true"
    }
  },
  "flex-layout.col#image": {
    "children": ["image"]
  },
  "flex-layout.col#message": {
    "children": ["message"]
  },
  "flex-layout.row#message": {
    "children": ["message"]
  },
  "flex-layout.col#container": {
    "children": ["flex-layout.row#container2", "flex-layout.col#message"],
    "props": {
      "width": "grow"
    }
  },
  "flex-layout.col#container.mobile": {
    "children": [
      "flex-layout.row#top.mobile",
      "flex-layout.row#quantity-selector",
      "flex-layout.row#unit-price",
      "flex-layout.row#price",
      "flex-layout.row#message"
    ],
    "props": {
      "width": "grow"
    }
  },
  "flex-layout.row#top.mobile": {
    "children": [
      "flex-layout.col#product-description",
      "flex-layout.col#remove-button"
    ],
    "props": {
      "colSizing": "auto",
      "preserveLayoutOnMobile": "true"
    }
  },
  "flex-layout.row#container2": {
    "children": [
      "flex-layout.col#product-description",
      "flex-layout.col#quantity",
      "flex-layout.col#price",
      "flex-layout.col#remove-button"
    ],
    "props": {
      "preserveLayoutOnMobile": "true",
      "colSizing": "auto"
    }
  },
  "flex-layout.col#product-description": {
    "children": [
      "flex-layout.row#product-brand",
      "flex-layout.row#product-name",
      "flex-layout.row#product-variations"
    ],
    "props": {
      "width": "grow"
    }
  },
  "flex-layout.row#product-brand": {
    "children": ["product-brand"],
    "props": {
      "marginBottom": "2"
    }
  },
  "flex-layout.row#product-name": {
    "children": ["product-name"]
  },
  "flex-layout.row#product-variations": {
    "children": ["product-variations"],
    "props": {
      "marginTop": "2"
    }
  },
  "flex-layout.col#quantity": {
    "children": [
      "flex-layout.row#quantity-selector",
      "flex-layout.row#unit-price"
    ]
  },
  "flex-layout.row#quantity-selector": {
    "children": ["quantity-selector"],
    "props": {
      "preventHorizontalStretch": "true"
    }
  },
  "flex-layout.row#unit-price": {
    "children": ["unit-price"],
    "props": {
      "preventHorizontalStretch": "true"
    }
  },
  "flex-layout.col#price": {
    "children": ["price"],
    "props": {
      "preventHorizontalStretch": "true"
    }
  },
  "flex-layout.row#price": {
    "children": ["price"],
    "props": {
      "preventHorizontalStretch": "true"
    }
  },
  "flex-layout.col#remove-button": {
    "children": ["remove-button"]
  }
}
